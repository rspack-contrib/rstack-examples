{
  "name": "rstack-examples",
  "version": "1.0.0",
  "description": "Repository for examples of rstack",
  "license": "MIT",
  "scripts": {
    "build": "npm run build:rspack && npm run build:rsbuild && npm run build:rspress && npm run build:rsdoctor && npm run build:rslib",
    "build:rsbuild": "pnpm --filter \"@rsbuild-example/*\" --stream build",
    "build:rsdoctor": "pnpm --filter \"@rsdoctor-example.*\" --stream build",
    "build:rslib": "pnpm --filter \"@rslib-example/-*\" --stream build",
    "build:rspack": "pnpm --filter \"@rspack-example/*\" --stream build",
    "build:rspress": "pnpm --filter \"@rspress-example/*\" --stream build",
    "bump": "taze -l",
    "prepare": "husky",
    "sort-package-json": "npx sort-package-json \"rspack/*/package.json\" \"rsbuild/*/package.json\" \"rspress/*/package.json\" \"rsdoctor/*/package.json\" \"rslib/*/package.json\" \"rstest/*/package.json\"",
    "test:rspack": "pnpm --filter \"@rspack-example/*\" --stream test",
    "test:rstest": "pnpm --filter \"@rstest-example/*\" --stream test"
  },
  "lint-staged": {
    "*.{js,ts,cjs,mjs,d.cts,d.mts,jsx,tsx,json,jsonc}": [
      "npx @biomejs/biome check --write --no-errors-on-unmatched"
    ]
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.13",
    "cross-env": "10.1.0",
    "husky": "9.1.7",
    "lint-staged": "16.2.7",
    "taze": "^19.9.2"
  },
  "packageManager": "pnpm@10.28.2"
}
